{ "terraform" :
  {
    "provider": {
        "aws": {
            "region": "us-east-1"
        }
      }
    },

    "resource": {
        "aws_instance": {
            "PubInst": {
                "count": "1",
                "subnet_id" : "${aws_subnet.us-east-1a-public.id}",
                "instance_type": "t2.medium",
                "ami": "ami-0922553b7b0369273",
                "key_name": "IMP",
                "associate_public_ip_address": true,
                "user_data" : "${filepath {\"IMP.pem\"}}",
                "security_groups" : "${aws_security_group.default.id}",
                     "root_block_device": {
                    "delete_on_termination": "true",
                    "volume_size": "50",
                    "volume_type": "gp2"
                },
                "tags": {
                    "Name": "HOSTNAME-01-01"
                }
            }
        }
      },
    "resource" :{"aws_default_security_group" :{"default" : {
"vpc_id" : "${aws_vpc.IMP_vpc.id}",
"name" : "Pub_Inst",
"description" : "SG for Pub_Inst",
"tags" : {
 "Name" : "Pub_Sec"
},
"ingress": {
  "protocol"  : -1,
  "self"      : true,
  "from_port" : 0,
  "to_port"   : 0
},
"egress" :{
  "from_port"   : 0,
  "to_port"     : 0,
  "protocol"    : "-1",
  "cidr_blocks" : ["0.0.0.0/0"]
}
}
  }
}
    }
